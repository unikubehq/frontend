{
  "name": "unikube-frontend",
  "version": "1.4.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "NODE_ENV='e2e' nyc --reporter=lcov --reporter=text vue-cli-service test:e2e",
    "test:e2e:ci": "NODE_ENV='e2e' nyc --reporter=lcov --reporter=text vue-cli-service test:e2e --headless --browser chrome",
    "test:report": "nyc report --reporter=text",
    "lint": "vue-cli-service lint",
    "codegen": "graphql-codegen --config codegen.yml",
    "test:codegen": "graphql-codegen --config tests/graphql/codegen.yml -e",
    "kindify": "node kindify.js",
    "release": "dotenv release-it"
  },
  "dependencies": {
    "@apollo/client": "^3.3.18",
    "@casl/ability": "^5.4.3",
    "@casl/vue": "^1.2.2",
    "@sentry/browser": "^5.30.0",
    "@sentry/vue": "^6.13.3",
    "axios": "^0.21.1",
    "core-js": "^3.12.1",
    "graphql": "^15.6.1",
    "graphql-tag": "^2.12.4",
    "jwt-decode": "^3.1.2",
    "keycloak-js": "^15.0.2",
    "monaco-editor": "^0.28.1",
    "monaco-editor-webpack-plugin": "^4.1.1",
    "monaco-yaml": "^3.0.0",
    "vue": "^2.6.11",
    "vue-apollo": "^3.0.7",
    "vue-axios": "^3.2.4",
    "vue-class-component": "^7.2.3",
    "vue-i18n": "^8.26.5",
    "vue-property-decorator": "^8.4.2",
    "vue-router": "^3.5.2",
    "vue2-dropzone": "^3.6.0",
    "vuelidate": "^0.7.6",
    "vuetify": "^2.5.9",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/runtime": "^7.15.4",
    "@cypress/code-coverage": "^3.9.11",
    "@graphql-codegen/add": "^3.1.0",
    "@graphql-codegen/cli": "2.2.1",
    "@graphql-codegen/schema-ast": "^2.2.0",
    "@graphql-codegen/typescript": "2.2.4",
    "@graphql-codegen/typescript-document-nodes": "2.1.6",
    "@graphql-codegen/typescript-operations": "2.1.8",
    "@intlify/vue-i18n-loader": "^1.1.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@sentry/webpack-plugin": "^1.18.3",
    "@types/chai": "^4.2.22",
    "@types/dropzone": "^5.7.4",
    "@types/jest": "^24.0.19",
    "@types/mocha": "^9.0.0",
    "@types/vuelidate": "^0.7.15",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.24.0",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-e2e-cypress": "^5.0.0-beta.1",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-plugin-router": "^4.5.13",
    "@vue/cli-plugin-typescript": "^4.5.15",
    "@vue/cli-plugin-unit-jest": "^4.5.13",
    "@vue/cli-plugin-vuex": "^4.5.14",
    "@vue/cli-service": "^4.5.13",
    "@vue/eslint-config-airbnb": "^5.3.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^1.2.2",
    "auto-changelog": "^2.3.0",
    "babel-plugin-istanbul": "^6.1.1",
    "cache-loader": "^4.1.0",
    "chai": "^4.3.4",
    "cypress": "^8.5.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-vue": "^7.20.0",
    "istanbul-lib-coverage": "^3.2.0",
    "lint-staged": "^11.2.4",
    "mock-apollo-client": "^1.1.0",
    "nyc": "^15.1.0",
    "release-it": "^14.11.6",
    "sass": "^1.43.3",
    "sass-loader": "^10.2.0",
    "typescript": "^3.9.9",
    "vue-cli-plugin-vuetify": "^2.0.9",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.7.3"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "auto-changelog": {
    "template": "keepachangelog"
  }
}
