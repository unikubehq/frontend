{
  "name": "unikube-frontend",
  "version": "1.5.4",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "NODE_ENV='e2e' CYPRESS_coverage=true nyc --reporter=lcov --reporter=text vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "codegen": "graphql-codegen --config codegen.yml",
    "kindify": "node kindify.js",
    "release": "dotenv release-it",
    "test:codegen": "graphql-codegen --config tests/graphql/codegen.yml -e",
    "test:e2e:ci": "NODE_ENV='e2e' CYPRESS_coverage=true nyc --reporter=lcov --reporter=text vue-cli-service test:e2e --headless --browser chrome",
    "test:report": "nyc report --reporter=text"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@casl/ability": "^5.4.3",
    "@casl/vue": "^2.1.2",
    "@sentry/vue": "^7.18.0",
    "@vue/apollo-composable": "^4.0.0-alpha.16",
    "@vue/apollo-option": "^4.0.0-alpha.16",
    "@vuelidate/core": "^2.0.0-alpha.40",
    "@vuelidate/validators": "^2.0.0-alpha.28",
    "axios": "^0.26.1",
    "core-js": "^3.22.0",
    "graphql": "^16.2.0",
    "graphql-tag": "^2.12.6",
    "jwt-decode": "^3.1.2",
    "keycloak-js": "^16.1.0",
    "monaco-editor": "^0.33.0",
    "monaco-editor-webpack-plugin": "^7.0.1",
    "monaco-yaml": "^4.0.0-alpha.2",
    "pinia": "^2.0.12",
    "vue": "^3.2.33",
    "vue-apollo": "^3.1.0",
    "vue-axios": "^3.4.0",
    "vue-class-component": "^8.0.0-0",
    "vue-content-loader": "^2.0.1",
    "vue-i18n": "^9.2.0-beta.35",
    "vue-router": "^4.0.14",
    "vue2-dropzone": "^3.6.0",
    "vuetify": "^3.0.0-beta.1",
    "vuex": "^4.0.2",
    "webpack": "^5.72.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.17.9",
    "@cypress/code-coverage": "^3.9.12",
    "@cypress/webpack-preprocessor": "^5.11.1",
    "@graphql-codegen/add": "^3.1.1",
