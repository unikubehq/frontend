    "chai": "^4.3.4",
    "cypress": "^9.5.4",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.13.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^8.6.0",
    "istanbul-lib-coverage": "^3.2.0",
    "lint-staged": "^12.3.8",
    "nyc": "^15.1.0",
    "release-it": "^14.14.2",
    "sass": "^1.50.1",
    "sass-loader": "^12.4.0",
