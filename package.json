{
  "name": "unikube-frontend",
  "version": "1.4.4",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "NODE_ENV='e2e' CYPRESS_coverage=true nyc --reporter=lcov --reporter=text vue-cli-service test:e2e",
    "test:e2e:ci": "NODE_ENV='e2e' CYPRESS_coverage=true nyc --reporter=lcov --reporter=text vue-cli-service test:e2e --headless --browser chrome",
    "test:report": "nyc report --reporter=text",
    "lint": "vue-cli-service lint",
    "codegen": "graphql-codegen --config codegen.yml",
    "test:codegen": "graphql-codegen --config tests/graphql/codegen.yml -e",
    "kindify": "node kindify.js",
    "release": "dotenv release-it"
  },
  "dependencies": {
    "@apollo/client": "^3.5.6",
    "@casl/ability": "^5.4.3",
    "@casl/vue": "^1.2.2",
    "@sentry/vue": "^6.16.1",
    "axios": "^0.24.0",
    "core-js": "^3.20.0",
    "graphql": "^16.2.0",
    "graphql-tag": "^2.12.6",
    "jwt-decode": "^3.1.2",
    "keycloak-js": "^16.1.0",
    "monaco-editor": "^0.31.1",
    "monaco-editor-webpack-plugin": "^7.0.1",
    "monaco-yaml": "^4.0.0-alpha.0",
    "vue": "^2.6.14",
    "vue-apollo": "^3.1.0",
    "vue-axios": "^3.4.0",
    "vue-class-component": "^7.2.3",
    "vue-i18n": "^8.26.7",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.5.3",
    "vue2-dropzone": "^3.6.0",
    "vuelidate": "^0.7.7",
    "vuetify": "^2.6.1",
    "vuex": "^3.6.2",
    "webpack": "^5.65.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.16.5",
    "@cypress/code-coverage": "^3.9.12",
    "@cypress/webpack-preprocessor": "^5.11.0",
    "@graphql-codegen/add": "^3.1.0",
    "@graphql-codegen/cli": "2.3.0",
    "@graphql-codegen/schema-ast": "^2.4.0",
    "@graphql-codegen/typescript": "2.4.1",
    "@graphql-codegen/typescript-document-nodes": "2.2.1",
    "@graphql-codegen/typescript-operations": "2.2.1",
    "@intlify/vue-i18n-loader": "^3.3.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@sentry/webpack-plugin": "^1.18.3",
    "@types/chai": "^4.3.0",
    "@types/dropzone": "^5.7.4",
    "@types/jest": "^27.0.3",
    "@types/mocha": "^9.0.0",
    "@types/vuelidate": "^0.7.15",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vue/cli-plugin-babel": "^5.0.0-rc.1",
    "@vue/cli-plugin-e2e-cypress": "^5.0.0-rc.1",
    "@vue/cli-plugin-eslint": "^5.0.0-rc.1",
    "@vue/cli-plugin-router": "^5.0.0-rc.1",
    "@vue/cli-plugin-typescript": "^5.0.0-rc.1",
    "@vue/cli-plugin-unit-jest": "^5.0.0-rc.1",
    "@vue/cli-plugin-vuex": "^5.0.0-rc.1",
    "@vue/cli-service": "^5.0.0-rc.1",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^1.3.0",
    "auto-changelog": "^2.3.0",
    "babel-plugin-istanbul": "^6.1.1",
    "chai": "^4.3.4",
    "cypress": "^9.1.1",
    "dotenv-cli": "^4.1.1",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-vue": "^8.2.0",
    "istanbul-lib-coverage": "^3.2.0",
    "lint-staged": "^12.1.3",
    "nyc": "^15.1.0",
    "release-it": "^14.11.8",
    "sass": "^1.45.1",
    "sass-loader": "^12.4.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "vue-cli-plugin-vuetify": "^2.4.5",
    "vue-template-compiler": "^2.6.14",
    "vuetify-loader": "^1.7.3"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "auto-changelog": {
    "template": "keepachangelog"
  }
}
